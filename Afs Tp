    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-3050, -184, -1032)
end)

TpSection:NewButton("Tokyo Ghoul", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-1922, -183, -1585)
end)

TpSection:NewButton("Hunter X Hunter", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(1677, -183, 2275)
end)

TpSection:NewButton("Sword Art Online", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(332, -183, -520)
end)

TpSection:NewButton("Bleach", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-2836, -184, -3644)
end)

