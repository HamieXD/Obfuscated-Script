-- Creating Ui
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Afs Tp", "Serpent")


-- Creating Tab
local TpTab = Window:NewTab("Teleport")
local LocalPlayerTab = Window:NewTab("LocalPlayer")
local ToggleUiTab = Window:NewTab("Toggle Ui")

-- Creating Section
local TpSection = TpTab:NewSection("Telport")
local LocalPlayerSection = LocalPlayerTab:NewSection("LocalPlayer")
local ToggleUiSection = ToggleUiTab:NewSection("Toggle Ui")

repeat wait() until game:IsLoaded() and game:GetService("Players")
for i,v in pairs(getconnections(game.Players.LocalPlayer.Idled)) do
v:Disable()
end

-- Coding for Tp section

TpSection:NewButton("Dragon Ball", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-2384, -185, 439)
end)

TpSection:NewButton("Naruto", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-92, -183, 2606)
end)

TpSection:NewButton("Jojo", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(6, -184, -2401)
end)

TpSection:NewButton("One Piece", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(1711, -183, 298)
end)

TpSection:NewButton("My Hero Academia", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-1987, -185, 2040)
end)

TpSection:NewButton("Attack On Titan", " ", function()
    game:GetService('Players').LocalPlayer.Character:FindFirstChild('HumanoidRootPart').CFrame = CFrame.new(-713, -183, 546)
end)

